{
  "name": "infra",
  "version": "0.1.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy --all --require-approval never --progress events",
    "deploy:preview": "cdk deploy --all --require-approval never --progress events",
    "deploy:dev": "cdk deploy --all --context environment=dev --require-approval never --progress events",
    "deploy:prod": "cdk deploy --all --context environment=prod --progress events",
    "destroy": "cdk destroy --all",
    "synth": "cdk synth",
    "diff": "cdk diff"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.1",
    "aws-cdk": "2.1100.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "@aws-solutions-constructs/aws-cloudfront-s3": "^2.96.0",
    "aws-cdk-lib": "^2.233.0",
    "constructs": "^10.4.4"
  }
}
