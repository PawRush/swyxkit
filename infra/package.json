{
  "name": "@swyxkit/infra",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for Swyxkit",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy": "cdk deploy --require-approval never --progress events",
    "deploy:pipeline": "cdk deploy SwyxkitPipelineStack --context codeConnectionArn=arn:aws:codeconnections:us-east-1:492267476755:connection/b723259a-c57f-4245-9416-a59676b72429 --context repositoryName=PawRush/swyxkit --context branchName=main --require-approval never --progress events",
    "destroy": "cdk destroy",
    "destroy:pipeline": "cdk destroy SwyxkitPipelineStack --context codeConnectionArn=arn:aws:codeconnections:us-east-1:492267476755:connection/b723259a-c57f-4245-9416-a59676b72429"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "aws-cdk-lib": "^2.234.0",
    "constructs": "^10.4.4",
    "typescript": "^5.3.2",
    "cdk-nag": "^2.26.0"
  },
  "engines": {
    "node": ">=16.14"
  },
  "type": "module"
}
